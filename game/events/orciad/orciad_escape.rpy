        jump ulganHelp
        
    "Обратить внимание на периметр." if delane_status == "tent" and delane_plan == True and delaneEscapePerimeter < 1:
        $ released_fix_rollback()
        jump observePerimeter
    
    "Обратитесь к Матрон за помощью." if (delane_status == "tent" and delane_plan == True and delaneDistraction == False and orcMatronState == 2 and delaneEscapePerimeter < 1) or  (delane_status == "tent" and delane_plan == True and delaneDistraction == False and orcMatronState == 3 and delaneEscapePerimeter < 1):
